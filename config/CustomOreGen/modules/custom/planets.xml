<IfModInstalled name="advancedrocketry">
    <IfCondition condition=':=dimension.generator.class="ChunkProviderPlanet"'>
        <IfModInstalled name='mekanism'>
            <Veins name='mekaOsmiumVeins'  inherits='PresetLayeredVeins' branchType='Bezier' drawWireframe='false' wireframeColor='0x60E78125' drawBoundBox='false' boundBoxColor='0x60E3F2F7'>
                <Description>
                    Small, fairly rare motherlodes with 2-4 horizontal veins each.
                </Description>
                <OreBlock block='mekanism:oreblock' weight='1.0' />
                <ReplacesOre block='stone' weight='1.0' />
                <ReplacesOre block='stoneLimestone' weight='1.0'/>
                <ReplacesOre block='stoneMarble' weight='1.0'/>
                <ReplacesOre block='stoneGranite' weight='1.0'/>
                <ReplacesOre block='stoneDiorite' weight='1.0'/>
                <ReplacesOre block='stoneAndesite' weight='1.0'/>
                <ReplacesOre block='stoneBasalt' weight='1.0'/>
                <ReplacesOre block='stoneSarsen' weight='1.0'/>
                <ReplacesOre block='stoneBlue' weight='1.0'/>
                <ReplacesOre block='stoneOrtho' weight='1.0'/>
                <ReplacesOre block='stoneSchist' weight='1.0'/>
                <ReplacesOre block='stoneCarbonate' weight='1.0'/>
                <ReplacesOre block='stoneFerro' weight='1.0'/>
                <Biome name='.*'  />
                <Setting name='MotherlodeFrequency'   avg=':= 1.416 * _default_ ' range=':= 1.416 * _default_ ' type='normal' scaleTo='base' />
                <Setting name='MotherlodeSize'        avg=':= 1.060 * _default_ ' range=':= 1.060 * _default_ ' type='normal' />
                <Setting name='MotherlodeHeight'      avg=':= 52 '                range=':= 16 '                type='uniform' scaleTo='base' />
                <Setting name='MotherlodeRangeLimit'  avg=':= _default_ '         range=':= _default_ '         type='normal' />
                <Setting name='BranchFrequency'       avg=':= _default_ '         range=':= _default_ '         type='normal' />
                <Setting name='BranchInclination'     avg=':= _default_ '         range=':= _default_ '         type='normal' />
                <Setting name='BranchLength'          avg=':= 1.190 * _default_ ' range=':= 1.190 * _default_ ' type='normal' />
                <Setting name='BranchHeightLimit'     avg=':= _default_ '         range=':= _default_ '         type='normal' scaleTo='base' />
                <Setting name='SegmentForkFrequency'  avg=':= _default_ '         range=':= _default_ '         type='normal' />
                <Setting name='SegmentForkLengthMult' avg=':= _default_ '         range=':= _default_ '         type='normal' />
                <Setting name='SegmentLength'         avg=':= 0.750 * _default_ ' range=':= 0.750 * _default_ ' type='normal' />
                <Setting name='SegmentAngle'          avg=':= _default_ '         range=':= _default_ '         type='normal' />
                <Setting name='SegmentPitch'          avg=':= _default_ '         range=':= _default_ '         type='normal' />
                <Setting name='SegmentRadius'         avg=':= 1.991 * _default_ ' range=':= 1.991 * _default_ ' type='normal' />
                <Setting name='OreDensity'            avg=':= _default_ '         range=':= _default_ '         type='normal' />
                <Setting name='OreRadiusMult'         avg=':= _default_ '         range=':= _default_ '         type='normal' />
            </Veins>
        </IfModInstalled>
        <IfModInstalled name='advancedrocketry--'>
            <Veins name='arokDilithiumVeins'  inherits='PresetLayeredVeins' branchType='Bezier' drawWireframe='false' wireframeColor='0x60E78125' drawBoundBox='false' boundBoxColor='0x60E3F2F7'>
                <Description>
                    Small, fairly rare motherlodes with 2-4 horizontal veins each.
                </Description>
                <OreBlock block='advancedrocketry:ore' weight='1.0' />
                <ReplacesOre block='stone' weight='1.0' />
                <ReplacesOre block='stoneLimestone' weight='1.0'/>
                <ReplacesOre block='stoneMarble' weight='1.0'/>
                <ReplacesOre block='stoneGranite' weight='1.0'/>
                <ReplacesOre block='stoneDiorite' weight='1.0'/>
                <ReplacesOre block='stoneAndesite' weight='1.0'/>
                <ReplacesOre block='stoneBasalt' weight='1.0'/>
                <ReplacesOre block='stoneSarsen' weight='1.0'/>
                <ReplacesOre block='stoneBlue' weight='1.0'/>
                <ReplacesOre block='stoneOrtho' weight='1.0'/>
                <ReplacesOre block='stoneSchist' weight='1.0'/>
                <ReplacesOre block='stoneCarbonate' weight='1.0'/>
                <ReplacesOre block='stoneFerro' weight='1.0'/>
                <Biome name='.*'  />
                <Setting name='MotherlodeFrequency'   avg=':= 1.416 * _default_ ' range=':= 1.416 * _default_ ' type='normal' scaleTo='base' />
                <Setting name='MotherlodeSize'        avg=':= 1.060 * _default_ ' range=':= 1.060 * _default_ ' type='normal' />
                <Setting name='MotherlodeHeight'      avg=':= 52 '                range=':= 16 '                type='uniform' scaleTo='base' />
                <Setting name='MotherlodeRangeLimit'  avg=':= _default_ '         range=':= _default_ '         type='normal' />
                <Setting name='BranchFrequency'       avg=':= _default_ '         range=':= _default_ '         type='normal' />
                <Setting name='BranchInclination'     avg=':= _default_ '         range=':= _default_ '         type='normal' />
                <Setting name='BranchLength'          avg=':= 1.190 * _default_ ' range=':= 1.190 * _default_ ' type='normal' />
                <Setting name='BranchHeightLimit'     avg=':= _default_ '         range=':= _default_ '         type='normal' scaleTo='base' />
                <Setting name='SegmentForkFrequency'  avg=':= _default_ '         range=':= _default_ '         type='normal' />
                <Setting name='SegmentForkLengthMult' avg=':= _default_ '         range=':= _default_ '         type='normal' />
                <Setting name='SegmentLength'         avg=':= 0.750 * _default_ ' range=':= 0.750 * _default_ ' type='normal' />
                <Setting name='SegmentAngle'          avg=':= _default_ '         range=':= _default_ '         type='normal' />
                <Setting name='SegmentPitch'          avg=':= _default_ '         range=':= _default_ '         type='normal' />
                <Setting name='SegmentRadius'         avg=':= 1.991 * _default_ ' range=':= 1.991 * _default_ ' type='normal' />
                <Setting name='OreDensity'            avg=':= _default_ '         range=':= _default_ '         type='normal' />
                <Setting name='OreRadiusMult'         avg=':= _default_ '         range=':= _default_ '         type='normal' />
            </Veins>
        </IfModInstalled>
        <IfModInstalled name="fp">
            <cloud name='fupaPlanetAlutineCloud'  inherits='PresetStrategicCloud' drawWireframe='false' wireframeColor='0x60C0C0FF' drawBoundBox='false' boundBoxColor='0x60C0C0FF'>
                <Description>
                    Create a phantom "cloud" where nothing spawns
                    Then create a group of flowers on the surface.
                </Description>
                <Biome name='.*' />
                <OreBlock block='fp:alutine_sand' weight='1.0' />
                <Setting name='CloudRadius'           avg=':= 0.005 * _default_ ' range=':= 0.005 * _default_ ' type='normal' />
                <Setting name='CloudThickness'        avg=':= 0.005 * _default_ ' range=':= 0.005 * _default_ ' type='normal' scaleTo='base' />
                <Setting name='DistributionFrequency' avg=':= 20.00 * _default_ ' range=':= 20.00 * _default_ ' type='normal' scaleTo='base' />
                <Setting name='CloudHeight'           avg=':= 70 '                range=':= 20 '                type='normal' scaleTo='base' />
                <Setting name='ParentRangeLimit'      avg=':= _default_ '         range=':= _default_ '         type='normal' />
                <Setting name='CloudSizeNoise'        avg=':= _default_ '         range=':= _default_ '         type='normal' />
                <Setting name='CloudInclination'      avg=':= _default_ '         range=':= _default_ '         type='normal' />
                <Setting name='OreDensity'            avg=':= _default_ '         range=':= _default_ '         type='normal' scaleTo='base' />
                <Setting name='OreVolumeNoiseCutoff'  avg=':= _default_ '         range=':= _default_ '         type='normal' />
                <Setting name='OreRadiusMult'         avg=':= _default_ '         range=':= _default_ '         type='normal' />
                <Veins name='fupaDDNeonBlocks' branchType='Bezier'  inherits='PresetLayeredVeins' drawWireframe='false' wireframeColor='0x60C0C0FF' drawBoundBox='false' boundBoxColor='0x60C0C0FF'>
                    <Description>
                        Nope.
                    </Description>
                    <OreBlock block='fp:neon_sand' weight='1.0' />
                    <!-- <PlacesBelow block='minecraft:air' weight='1.0' /> -->
                    <Replaces block='minecraft:cobblestone' weight='1.0' />
                    <Replaces block='minecraft:stone' weight='1.0' />
                    <Replaces block='minecraft:sand' weight='1.0' />
                    <Setting name='MotherlodeSize'        avg=':= 1.922 * _default_ ' range=':= 1.922 * _default_ ' type='normal' />
                    <Setting name='MotherlodeRangeLimit'  avg=' 20 '                  range=' 5 '                   type='uniform' />
                    <Setting name='MotherlodeHeight'      avg='64'                    range='10'                    scaleTo='biome' />
                    <Setting name='MotherlodeFrequency'   avg=':= 2.222 * _default_ ' range=':= 2.222 * _default_ ' type='normal' scaleTo='base' />
                    <Setting name='BranchFrequency'       avg=':= 0 '                 range=':= 0 '                 type='normal' />
                </Veins>
            </cloud>
        </IfModInstalled>
    </IfCondition>
</IfModInstalled>
