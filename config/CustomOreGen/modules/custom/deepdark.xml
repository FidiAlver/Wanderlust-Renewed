
<IfModInstalled name="extrautils2">
    <ConfigSection>
        <IfCondition condition=':=dimension.generator.class="ChunkProviderUnderdark"'>
            <!-- Iron Veins -->
            <Veins name='vaniIronDDVeins'  inherits='PresetLayeredVeins' branchType='Bezier' drawWireframe='true' wireframeColor='0x60CC7722' drawBoundBox='false' boundBoxColor='0x60CC7722'>
                <Description>
                    Small, fairly rare motherlodes with 2-4 horizontal veins each.
                </Description>
                <OreBlock block='minecraft:iron_ore' weight='1.0' />
                <ReplacesOre block='stone'           weight='1.0' />
                <ReplacesOre block='cobblestone'     weight='1.0' />
                <Biome name='.*'  />
                <Setting name='MotherlodeFrequency'   avg=':= 2.000 * _default_ ' range=':= 2.000 * _default_ ' type='normal' scaleTo='base' />
                <Setting name='MotherlodeSize'        avg=':= 1.600 * _default_ ' range=':= 1.600 * _default_ ' type='normal' />
